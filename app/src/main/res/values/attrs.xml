<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Battery">
        <attr name="batteryColor" format="color"/>
        <attr name="batteryPower" format="integer"/>
        <attr name="batteryStatus" format="enum">
            <enum name="battery_loss" value="1"/>
            <enum name="battery_charge" value="2"/>
            <enum name="battery_work" value="3"/>
        </attr>
    </declare-styleable>
</resources>